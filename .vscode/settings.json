{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  },
  "git.ignoreLimitWarning": true,
  "sonarlint.connectedMode.project": {
    "connectionId": "swipedevuser",
    "projectKey": "SwipeDevUser_swipesavvy-mobile-app"
  },
  "files.exclude": {
    "**/android": true,
    "**/ios": true
  },
  "java.autobuild.enabled": false,
  "java.import.gradle.enabled": false,
  "java.import.maven.enabled": false,
  "java.import.exclusions": ["**/android/**", "**/ios/**"],
  "java.project.resourceFilters": ["**/android/**", "**/ios/**"],
  "java.configuration.updateBuildConfiguration": "disabled",
  "kotlin.languageServer.enabled": false,
  "gradle.autoDetect": "off",
  "gradle.nestedProjects": false,
  "gradle.javaDebug.cleanOutput": false,
  "files.watcherExclude": {
    "**/android/**": true,
    "**/ios/**": true,
    "**/node_modules/**": true
  }
}
