{
  "expo": {
    "owner": "swipesavvy2026",
    "name": "SwipeSavvy",
    "slug": "swipesavvy-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "swipesavvy",
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swipesavvy.mobileapp",
      "buildNumber": "32",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow SwipeSavvy to use Face ID for secure authentication",
        "NSLocationWhenInUseUsageDescription": "Allow SwipeSavvy to access your location for geo-based promotions",
        "NSMicrophoneUsageDescription": "Allow SwipeSavvy to use your microphone for voice commands",
        "NSCameraUsageDescription": "Allow SwipeSavvy to use your camera to scan checks",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF8DC"
      },
      "package": "com.swipesavvy.mobileapp",
      "versionCode": 1,
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-secure-store",
      "expo-local-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-router",
        {
          "root": "./app"
        }
      ]
    ],
    "extra": {
      "MOCK_API": "false",
      "API_BASE_URL": "https://api.swipesavvy.com/api/v1",
      "AI_API_BASE_URL": "https://api.swipesavvy.com/api/v1",
      "WS_URL": "wss://api.swipesavvy.com/ws",
      "DEBUG_MODE": "false",
      "eas": {
        "projectId": "df952ef6-f2ed-4931-a00c-1ee3d29f639b"
      },
      "router": {
        "root": "./app"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/df952ef6-f2ed-4931-a00c-1ee3d29f639b"
    }
  }
}
